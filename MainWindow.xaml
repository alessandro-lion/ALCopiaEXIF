<Window x:Class="ALCopiaEXIF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ALCopiaEXIF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
    xmlns:System="clr-namespace:System;assembly=System.Runtime">
    <Window.Resources>
        <System:String x:Key="LabelStatus">Stato esecuzione programma</System:String>
    </Window.Resources>
    <Grid Background="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}">
        <Label Content="LABEL" Margin="10,396,10,10" d:LayoutOverrides="HorizontalAlignment" Background="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}"/>
        <Label Content="{DynamicResource LabelStatus}" Margin="10,363,10,43" d:LayoutOverrides="HorizontalAlignment"/>

    </Grid>
</Window>
